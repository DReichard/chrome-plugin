{
    "name": "Inquest background extension",
    "version": "0.1",
    "description": "TBD",
    "author": "TBD",
    "short_name": "Let nothing through",
    "permissions": ["storage", "activeTab"],
    "content_scripts": [
        {
          "matches": [ "<all_urls>" ],
          "js": [ "pageHook.js" ]
        }
    ],
    "background": {
      "scripts": [ "background.js"],
      "persistent": false
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "manifest_version": 2
  }